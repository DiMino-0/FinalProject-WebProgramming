name: Retrieve Deployments
on:
  push:
    branches:
      -main
jobs:
  accessRenderEndpoint: >- curl --request GET \
     --url 'https://api.render.com/v1/services/${RENDER_SERVICE_KEY}/deploys?status=live&limit=20' \
     --header 'accept: application/json' \
     --header 'authorization: Bearer ${RENDER_API_KEY}'
